@model IEnumerable<WebApplication1.Models.TrashViewModel>

@{
    ViewBag.Title = "JsonResult";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Raport</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Klasy złomu")
        </th>
        <th>
            @Html.DisplayName("Ilości")
        </th>
        <th>
            @Html.DisplayName("Uwagi")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.EditorFor(modelItem => item.someOption)
        </td>
        <td>
            @Html.EditorFor(modelItem => item.someText)
        </td>
        <td>
            @Html.EditorFor(modelItem => item.someNumber)
        </td>
       
    </tr>
}

</table>
