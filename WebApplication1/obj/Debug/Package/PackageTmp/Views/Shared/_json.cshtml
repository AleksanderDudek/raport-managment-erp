@model IEnumerable<WebApplication1.Models.TrashViewModel>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.someOption)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.someText)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.someNumber)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.someOption)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.someText)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.someNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
